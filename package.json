{
  "name": "zksync-sso-contracts-7579",
  "version": "0.1.0",
  "description": "ERC-7579 compatible ZKsync SSO contracts",
  "main": "index.js",
  "scripts": {
    "bundler": "alto --config ./alto.json",
    "test": "node --test --import tsx --test-concurrency=1 test/integration/*.test.ts",
    "sign_hash_with_passkey": "pnpm tsx test/integration/utils.ts --hash",
    "anvil": "anvil --fork-url https://reth-ethereum.ithaca.xyz/rpc --chain-id 1337 -vvvvv",
    "deploy": "forge script script/Deploy.s.sol",
    "//": "Uses the last private key of anvil's rich accounts list",
    "deploy-test": "forge script script/Deploy.s.sol --rpc-url http://127.0.0.1:8545 --broadcast --private-key 0x2a871d0798f97d79848a013d4936a73bf4cc922c825d33c1cf7073dff6d409c6 --sig 'deployAll()'"
  },
  "keywords": [
    "zksync",
    "account-abstraction",
    "ERC7579"
  ],
  "author": "Matter Labs <zksync.io>",
  "license": "MIT",
  "packageManager": "pnpm@10.17.1",
  "devDependencies": {
    "@pimlico/alto": "^0.0.19",
    "@types/node": "^24.3.0",
    "tsx": "^4.20.5",
    "viem": "^2.36.0"
  }
}
